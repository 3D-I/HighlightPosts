{
	"name": "threedi/hlposts",
	"type": "phpbb-extension",
	"description": "Highlight posts and mark them as read system, useful for Educational Boards.",
	"homepage": "https://phpbbstudio.com",
	"version": "2.0.3-RC",
	"time": "2019-10-02",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "phpBB Studio",
			"email": "info@phpbbstudio.com",
			"homepage": "https://www.phpbbstudio.com",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.5",
		"composer/installers": "~1.0"
	},
	"extra": {
		"display-name": "phpBB Studio - Highlight Posts",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.1,<3.3.0@dev"
		},
		"version-check": {
			"host": "3d-i.github.io",
			"directory": "/site/vchecks",
			"filename": "hlposts-vcheck.json"
		}
	}
}

	"require": {
		"php": ">=5.5",
		"composer/installers": "~1.0"
	},
	"extra": {
		"display-name": "Highlight Posts",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.1,<3.3.0@dev"

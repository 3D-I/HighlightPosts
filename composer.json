	"homepage": "https://github.com/3D-I/HighlightPosts",
	"version": "2.0.0-RC",
	"time": "2018-04-17",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "3Di",
			"email": "eataly3d@gmail.com",
			"homepage": "http://3di.space",
			"role": "Lead Developer"
		},
		{
			"name": "Mr. Goldy",
			"role": "Developer"
		}
